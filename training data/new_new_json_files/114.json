    "bbox": [
      44.6,
      182.9,
      539.4,
      195.6
    ],
    "font_size": 11.5,
    "font_name": "TimesNewRomanPSMT",
    "label": "BODY"
  },
  {
    "pdf_id": "114",
    "page_number": 1,
    "block_id": "114_p1_b011",
