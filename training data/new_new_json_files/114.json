    "bbox": [
      44.6,
      308.0,
      528.4,
      320.7
    ],
    "font_size": 11.5,
    "font_name": "TimesNewRomanPS-BoldMT",
    "label": "BODY"
  },
  {
    "pdf_id": "114",
