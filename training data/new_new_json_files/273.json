    "page_number": 1,
    "block_id": "273_p1_b025",
    "text": "Temporary Assistance for Needy Families (TANF) funds for cash aid and food stamps for a",
    "bbox": [
      72.0,
      425.8,
      516.3,
      439.1
    ],
    "font_size": 12.0,
    "font_name": "TimesNewRomanPSMT",
    "label": "BODY"
  },
  {
