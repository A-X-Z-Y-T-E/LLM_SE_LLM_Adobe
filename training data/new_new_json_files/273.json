    "bbox": [
      72.0,
      593.6,
      501.6,
      606.9
    ],
    "font_size": 12.0,
    "font_name": "TimesNewRomanPSMT",
    "label": "BODY"
  },
  {
    "pdf_id": "273",
    "page_number": 1,
    "block_id": "273_p1_b036",
