  {
    "pdf_id": "24",
    "page_number": 2,
    "block_id": "24_p2_b006",
    "text": "were required to use a passport or another secure document at the border, a requirement of the",
    "bbox": [
      42.8,
      238.9,
      550.4,
      258.1
    ],
    "font_size": 12.0,
    "font_name": "FreeSans",
    "label": "BODY"
