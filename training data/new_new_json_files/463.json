    "bbox": [
      156.0,
      707.3,
      456.1,
      716.3
    ],
    "font_size": 9.0,
    "font_name": "Melior",
    "label": "BODY"
  },
  {
    "pdf_id": "463",
    "page_number": 2,
    "block_id": "463_p2_b049",
