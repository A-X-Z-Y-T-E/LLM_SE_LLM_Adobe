    "page_number": 3,
    "block_id": "949_p3_b017",
    "text": "o Maintain the current program for those 55 and older (those who have long planned on the",
    "bbox": [
      42.8,
      628.9,
      527.6,
      648.1
    ],
    "font_size": 12.0,
    "font_name": "FreeSans",
    "label": "BODY"
  },
  {
    "pdf_id": "949",
    "page_number": 3,
    "block_id": "949_p3_b018",
    "text": "existing program);",
    "bbox": [
      42.8,
      643.9,
      142.2,
      663.1
    ],
    "font_size": 12.0,
    "font_name": "FreeSans",
    "label": "BODY"
  },
