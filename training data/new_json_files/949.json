    "bbox": [
      42.8,
      223.9,
      463.0,
      243.1
    ],
    "font_size": 12.0,
    "font_name": "FreeSans",
    "label": "BODY"
  },
  {
    "pdf_id": "949",
    "page_number": 4,
    "block_id": "949_p4_b006",
