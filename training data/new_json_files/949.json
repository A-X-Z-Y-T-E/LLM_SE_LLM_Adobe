    "bbox": [
      42.8,
      478.9,
      536.3,
      498.1
    ],
    "font_size": 12.0,
    "font_name": "FreeSans",
    "label": "BODY"
  },
  {
    "pdf_id": "949",
    "page_number": 3,
    "block_id": "949_p3_b015",
