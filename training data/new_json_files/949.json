    "bbox": [
      42.8,
      103.9,
      518.2,
      123.1
    ],
    "font_size": 12.0,
    "font_name": "FreeSans",
    "label": "BODY"
  },
  {
    "pdf_id": "949",
    "page_number": 3,
    "block_id": "949_p3_b003",
    "text": "efforts to stop the raid on the Social Security surplus and raise public awareness regarding the",
    "bbox": [
      42.8,
      118.9,
      543.0,
      138.1
    ],
    "font_size": 12.0,
    "font_name": "FreeSans",
    "label": "BODY"
  },
