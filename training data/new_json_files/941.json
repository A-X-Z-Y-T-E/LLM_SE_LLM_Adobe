  {
    "pdf_id": "941",
    "page_number": 1,
    "block_id": "941_p1_b005",
    "text": "Washington D.C. - Congressman John P. Murtha, Chairman of the House Appropriations Subcommittee on Defense,",
    "bbox": [
      28.3,
      268.0,
      544.7,
      281.7
    ],
