    "bbox": [
      42.8,
      178.9,
      547.7,
      198.1
    ],
    "font_size": 12.0,
    "font_name": "FreeSans",
    "label": "BODY"
  },
  {
    "pdf_id": "591",
    "page_number": 4,
    "block_id": "591_p4_b007",
    "text": "up my sleeves, and take care of our problems.",
